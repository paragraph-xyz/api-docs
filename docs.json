{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Paragraph",
  "colors": {
    "primary": "#3F64F8",
    "light": "#F6F4EE",
    "dark": "#26251A"
  },
  "favicon": "/favicon.svg",
  "redirects": [
    {
      "source": "/api-reference",
      "destination": "/api-reference/publications/get-publication-by-id"
    }
  ],
  "navigation": {
    "tabs": [
      {
        "tab": "Development",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "development/index"
            ]
          },
          {
            "group": "Typescript SDK",
            "pages": [
              "development/sdk/index",
              "development/sdk/examples"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "openapi": "/paragraph-api/openapi.json"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://paragraph.com/@blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/paragraph_wood.png",
    "dark": "/logo/paragraph_light.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@paragraph.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}